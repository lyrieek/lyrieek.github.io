{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "amd": true,
        "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "module",
        "ecmaFeatures": {
            "globalReturn": true,
            "impliedStrict": true,
            "modules": true
        }
    },
    "globals":{
        "_": true,
        "Promise": true
    },
    "rules": {
        "no-cond-assign": 2,
        "no-constant-condition": 2,
        "comma-dangle": [1, "never"],
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 1,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 1,
        "no-func-assign": 2,
        "no-inner-declarations": [2, "functions"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-prototype-builtins": 0,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,

        "accessor-pairs": 2,
        "array-callback-return": 0,
        "block-scoped-var": 0,
        "complexity": [2, 150],
        "consistent-return": 0,
        "default-case": 2,
        "dot-location": [2, "property"],
        "eqeqeq": [1, "allow-null"],
        "no-alert": 0,
        "no-caller": 2,
        "no-case-declarations": 2,
        "no-div-regex": 2,
        "no-else-return": 0,
        "no-empty-function": 1,
        "no-empty-pattern": 2,
        "no-eq-null": 1,
        "no-extra-bind": 2,
        "no-extra-label:": 0,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 0,
        "no-implicit-globals": 1,
        "no-implied-eval": 2,
        "no-invalid-this": 0,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-multi-spaces": 1,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 0,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 0,
        "no-proto": 2,
        "no-redeclare": 1,
        "no-return-assign": 0,
        "no-script-url": 0,
        "no-self-assign": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unmodified-loop-condition": 2,
        "no-unused-expressions": 0,
        "no-unused-labels": 2,
        "no-useless-call": 2,
        "no-useless-concat": 1,
        "no-useless-escape": 0,
        "no-void": 0,
        "no-warning-comments": 0,
        "no-with": 2,
        "vars-on-top": 0,
        "wrap-iife": [2, "any"],
        "yoda": [1, "never"],

        "init-declarations": 0,
        "no-catch-shadow": 0,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-restricted-globals": 0,
        "no-shadow": 0,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-undefined": 0,
        "no-unused-vars": [1, {
            "vars": "all",
            "args": "none"
        }],
        "no-use-before-define": 0,

        "array-bracket-spacing": [1, "never"],
        "block-spacing": [1, "never"],
        "brace-style": [1, "1tbs", {
            "allowSingleLine": true
        }],
        "comma-style": [1, "last"],
        "computed-property-spacing": [2, "never"],
        "func-names": 0,
        "eol-last": 2,
        "key-spacing": [1, {
            "beforeColon": false,
            "afterColon": true
        }],
        "linebreak-style": [1, "unix"],
        "func-style": 0,
        "max-nested-callbacks": [1, 5],
        "id-blacklist": 0,
        "id-length": 0,
        "id-match": 0,
        "jsx-quotes": 0,
        "max-len": [1, 5000],
        "max-lines": 0,
        "max-params": [1, 21],
        "max-statements": [1, 500],
        "max-statements-per-line": 0,
        "new-cap": [2, {
            "newIsCap": true,
            "capIsNew": false
        }],
        "new-parens": 2,
        "newline-after-var": 0,
        "no-array-constructor": 2,
        "no-bitwise": 0,
        "newline-before-return": 0,
        "no-continue": 0,
        "no-inline-comments": 0,
        "no-lonely-if": 0,
        "no-mixed-operators": 0,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": [1, {
            "max": 2
        }],
        "no-negated-condition": 0,
        "no-nested-ternary": 0,
        "no-new-object": 2,
        "no-plusplus": 0,
        "no-restricted-syntax": 0,
        "no-spaced-func": 2,
        "no-ternary": 0,
        "no-trailing-spaces": 2,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 1,
        "no-whitespace-before-property": 0,
        "object-curly-newline": 0,
        "object-curly-spacing": 0,
        "object-property-newline": 0,
        "one-var-declaration-per-line": 0,
        "operator-assignment": 0,
        "operator-linebreak": [1, "before", {
            "overrides": {
                "=": "after"
            }
        }],
        "padded-blocks": 0,
        "quote-props": 0,
        "semi-spacing": 0,
        "sort-vars": 0,
        "space-before-function-paren": [0, "always"],
        "space-in-parens": [1, "never"],
        "space-unary-ops": [2, {
            "words": true,
            "nonwords": false
        }],
        "unicode-bom": 0,
        "wrap-regex": 0,


        "arrow-body-style": 2,
        "arrow-parens": 2,
        "arrow-spacing": [2, {
            "before": true,
            "after": true
        }],
        "constructor-super": 0,
        "generator-star-spacing": [2, {
            "before": true,
            "after": true
        }],
        "no-class-assign": 2,
        "no-confusing-arrow": 0,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-duplicate-imports": 0,
        "no-new-symbol": 2,
        "no-restricted-imports": 0,
        "no-this-before-super": 2,
        "no-useless-computed-key": 0,
        "no-var": 0,
        "object-shorthand": 0,
        "prefer-arrow-callback": 0,
        "prefer-const": 0,
        "prefer-reflect": 0,
        "prefer-spread": 0,
        "prefer-template": 0,
        "prefer-rest-params": 0,
        "require-yield": 0,
        "rest-spread-spacing": 0,
        "sort-imports": 0,
        "template-curly-spacing": 1,
        "yield-star-spacing": 2
    }
}
